package net.luckywings.mobigame.server.service;

/**
 * ClassName: ServiceHandlerException
 * 
 * @description
 * @author nikm
 * @Date Feb 25, 2013
 * 
 */
public class ServiceHandlerException extends Exception {

    /**
     * @param message
     * @param cause
     */
    public ServiceHandlerException(String message, Throwable cause) {
        super(message, cause);
    }

    /**
     * @param message
     */
    public ServiceHandlerException(String message) {
        super(message);
    }

    /**
     * @param cause
     */
    public ServiceHandlerException(Throwable cause) {
        super(cause);
    }

    private static final long serialVersionUID = -6498674930426414653L;

    public ServiceHandlerException() {

    }

}
