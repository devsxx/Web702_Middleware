	private final int code;
	private final String comments;

	EnumFieldH5(final int code, final String comments) {
		this.code = code;
		this.comments = comments;
	}

	public static EnumFieldH5 fromCode(final int code) {
		for (EnumFieldH5 s : EnumFieldH5.values()) {
			if (s.code == code) {
				return s;
			}
		}
		return null;
	}
	
	public static EnumFieldH5 fromName(final String name) {
        for (EnumFieldH5 s : EnumFieldH5.values()) {
            if (s.name().equals(name)) {
                return s;
            }
        }
        return null;
    }

	public int getCode() {
		return code;
	}

	public String getComments() {
		return toString();
	}

	public String toString() {
		return comments;
