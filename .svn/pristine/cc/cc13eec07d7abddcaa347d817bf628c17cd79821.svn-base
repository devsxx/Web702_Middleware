# CAS server address
server.host.dataservice.cas=http://127.0.0.1:8880/mobigame-cas-service/service/cas

# Configure dynamic database number and priority 
server.database.dynamic.dbnum=2
server.database.dynamic.prioritylist=100,0

# Cache Config
# 1=com.danga.MemCached, 2=xmemcaced
server.cache.type=2
# hosts split with space, like 10.20.71.71:11211 10.20.70.203:11211
server.cache.hosts=127.0.0.1:11211 127.0.0.1:11211
server.cache.prefix=mw-data-
# default cache time in sec, 3600
server.cache.timeout=3600

# session config 
server.session.hosts=127.0.0.1:11211
# in sec, default 900=60*15
server.session.timeout=1800
server.session.prefix=mw-session-