package net.luckywings.mobigame.server.exception;

/**
 * ClassName: DataServiceException
 * @description
 * @author nikm
 * @Date   Mar 8, 2013
 *
 */
public class DataServiceException extends Exception {

	/**
	 * 
	 */
	private static final long serialVersionUID = 4208878626364087064L;

	/**
	 * 
	 */
	public DataServiceException() {
	}

	/**
	 * @param message
	 */
	public DataServiceException(String message) {
		super(message);
	}

	/**
	 * @param cause
	 */
	public DataServiceException(Throwable cause) {
		super(cause);
	}

	/**
	 * @param message
	 * @param cause
	 */
	public DataServiceException(String message, Throwable cause) {
		super(message, cause);
	}

}
